[
  {
    "cpu": 256,
    "essential": true,
    "image": "${image_url}",
    "memory": 512,
    "name": "${container_name}",
    "command": [
      "bundle",
      "exec",
      "rails",
      "s",
      "-p",
      "3000",
      "-b",
      "0.0.0.0"
    ],
    "portMappings": [
      {
        "containerPort": ${container_port},
        "hostPort": ${host_port}
      }
    ]
  }
]
