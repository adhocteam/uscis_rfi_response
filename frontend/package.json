{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://backend:3001",
  "dependencies": {
    "@cmsgov/design-system-core": "^1.10.0",
    "@cmsgov/design-system-layout": "^1.10.0",
    "node-sass": "^4.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-s3-uploader": "^4.6.2",
    "react-scripts": "1.1.0"
  },
  "scripts": {
    "copy-design-system": "cp -r node_modules/@cmsgov/design-system-core/fonts src/fonts && cp -r node_modules/@cmsgov/design-system-core/images src/images",
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "yarn run build-css && node-sass --include-path ./src --include-path ./node_modules src/*/*.scss -o src/ --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
